{
  "name": "Switch Tabs",
  "version": "1.0",
		
  "manifest_version": 2,
  "description": "Switch tabs usiing Alt+Q",
  
  "permissions": [
    "<all_urls>","tabs","activeTab"
  ],

  "background" : {
    "scripts" : ["background.js"]
  },
  
  "browser_action": {
    "default_icon": "icon.png",
	"default_title": "Switch Tabs"
  },
  "icons": {
      "128": "icon.png"
   },
  "commands": {
        "toggle": {
            "suggested_key": {
              "default": "Alt+Q",
              "mac": "Command+Q"
            },
            "description": "Toggle switch feature"
		}
	}
}